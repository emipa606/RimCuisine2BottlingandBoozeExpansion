  <ThingDef ParentName="BuildingBase">
    <defName>RC2_WineFermentingBarrel</defName>
    <label>wine fermenting barrel</label>
    <thingClass>ExpandableFermenting.Building_Fermenting</thingClass>
    <graphicData>
      <texPath>RC2_Things/RC2_Buildings/WineBarrel/WineBarrel</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <damageData>
        <rect>(0.05,0.1,0.9,0.9)</rect>
      </damageData>
      <shadowData>
        <volume>(0.75, 0.35, 0.33)</volume>
      </shadowData>
    </graphicData>
    <minifiedDef>MinifiedThing</minifiedDef>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <fillPercent>0.45</fillPercent>
    <pathCost>60</pathCost>
    <thingCategories>
      <li>BuildingsProduction</li>
    </thingCategories>
    <statBases>
      <WorkToBuild>600</WorkToBuild>
      <Mass>10</Mass>
      <MaxHitPoints>100</MaxHitPoints>
      <Flammability>1.0</Flammability>
    </statBases>
    <description>A barrel for fermenting grape must into wine.</description>
    <costList>
      <Steel>10</Steel>
      <WoodLog>30</WoodLog>
    </costList>
    <comps>
      <li Class="CompProperties_TemperatureRuinable">
        <minSafeTemperature>-1</minSafeTemperature>
        <maxSafeTemperature>32</maxSafeTemperature>
        <progressPerDegreePerTick>0.00001</progressPerDegreePerTick>
      </li>
      <li Class="ExpandableFermenting.CompProperties_Fermenting">
        <ingredientDef>RC2_GrapeMust</ingredientDef>
        <productDef>RC2_Wine</productDef>
        <!-- <capacity>25</capacity> is default -->
        <fermentationDuration>1800000</fermentationDuration>
        <!-- <minIdealTemperature>7.0</minIdealTemperature> is default. Beer is not ruined below this temperature, but progress slows. -->
      </li>
      <li Class="CompProperties_Forbiddable" />
    </comps>
    <tickerType>Normal</tickerType>
    <rotatable>true</rotatable>
    <designationCategory>RC2_RimCuisine</designationCategory>
    <constructEffect>ConstructWood</constructEffect>
    <researchPrerequisites>
      <li>Brewing</li>
    </researchPrerequisites>
  </ThingDef>
